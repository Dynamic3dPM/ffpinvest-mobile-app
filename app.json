{
  "expo": {
    "name": "ffpinvest",
    "slug": "ffpinvest",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./asset/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.timc82.ffpinvest",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./asset/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.timc82.ffpinvest"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./asset/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./asset/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "09c8dc39-015b-428b-93c2-c17a4aa167e0"
      }
    }
  }
}
